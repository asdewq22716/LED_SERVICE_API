function vr140_scroll(b){var b=b.data.direction,a=jQuery(this).parents(".nume-vr140"),c=jQuery(".nume-vr140").index(a),a=jQuery(this).parents(".nume-vr140");jQuery(".center",this).width();jQuery(".recos li",this).outerWidth();var d=vr140_curr_si[c]*jQuery(".recos li:nth-child(2)",a).outerWidth(!0),c=jQuery(a).index(".nume-vr140");"right"==b?(1==vr140_pos[c]&&(jQuery(".arrow-left-disabled",a).addClass("arrow-left").removeClass("arrow-left-disabled"),jQuery(".arrow-left",a).on("click",{direction:"left"},

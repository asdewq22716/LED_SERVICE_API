function vr140_resetScroll(b,a){var c=[];jQuery(".nume-vr140").each(function(d){var f=jQuery(".recos li",this).length/jQuery(".recos",this).length;!1==jQuery(this).hasClass("column")&&1!=f&&(jQuery(".recos",this).animate({"margin-left":"0"}),jQuery(".arrow-left",this).off(),jQuery(".arrow-right",this).off(),jQuery(".arrow-left",this).addClass("arrow-left-disabled").removeClass("arrow-left"),a>=jQuery(".recos li",this).length?(jQuery(".arrow-right",this).off(),jQuery(".arrow-right",this).addClass("arrow-right-disabled").removeClass("arrow-right")):

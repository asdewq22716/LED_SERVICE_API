function vr140_setCounters(b,a){jQuery(".nume-vr140").each(function(c){1!=a[c]?(jQuery(".pgn",this).html(b[c]+"/"+a[c]),jQuery(".arrow-left-disabled",this).show(),jQuery(".arrow-right",this).show()):(jQuery(".pgn",this).html(""),jQuery(".arrow-left",this).hide(),jQuery(".arrow-right",this).hide(),jQuery(".arrow-left-disabled",this).hide(),jQuery(".arrow-right-disabled",this).hide())})}function vr140_mover(){jQuery(".sms",this).css({visibility:"visible"})}
function vr140_mout(){jQuery(".sms",this).css({visibility:"hidden"})}function vr140_loadImg(b,a,c){jQuery(".recos",b).each(function(){jQuery("li",this).each(function(b){b<a*c&&jQuery(".imgt img",this).attr("src",jQuery(this).data("img"))})})}var vr140_curr_si=[],vr140_pos=[],vr140_max=[],vr140_is_init=!1;
